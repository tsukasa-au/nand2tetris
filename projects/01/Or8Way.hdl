/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=tmp00);
    Or(a=in[2], b=in[3], out=tmp01);
    Or(a=in[4], b=in[5], out=tmp02);
    Or(a=in[6], b=in[7], out=tmp03);
    Or(a=tmp00, b=tmp01, out=tmp10);
    Or(a=tmp02, b=tmp03, out=tmp11);
    Or(a=tmp10, b=tmp11, out=out);
}
